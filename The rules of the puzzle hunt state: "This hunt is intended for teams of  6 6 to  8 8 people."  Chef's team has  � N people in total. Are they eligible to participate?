import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner input=new Scanner(System.in);
		int n=input.nextInt();
		if(n>=6&&n<=8){
		    System.out.println("YES");
		}
		else{
		    System.out.println("NO");
		}

	}
}
