import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
	    Scanner input=new Scanner(System.in);
	    int n=input.nextInt();
	    for(int i=0;i<n;i++){
	        int a=input.nextInt();
	        int b=input.nextInt();
	        int c=input.nextInt();
	        int max=Math.max(c,Math.max(a,b));
	        int min=Math.min(c,Math.min(a,b));
	        System.out.println(max-min);
	    }

	}
}
