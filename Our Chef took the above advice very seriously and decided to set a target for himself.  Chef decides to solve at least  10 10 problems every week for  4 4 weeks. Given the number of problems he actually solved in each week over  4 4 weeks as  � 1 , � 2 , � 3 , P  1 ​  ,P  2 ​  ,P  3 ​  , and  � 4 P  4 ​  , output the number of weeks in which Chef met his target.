import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner input=new Scanner(System.in);
		int n=0;
		int a=input.nextInt();
		int b=input.nextInt();
		int c=input.nextInt();
		int d=input.nextInt();
		int j=0;
		if(a>=10){
		    j++;
		}
		if(b>=10){
		    j++;
		}
		if(c>=10){
		    j++;
		}
		if(d>=10){
		   j++;
		}
		System.out.println(j);

	}
}
