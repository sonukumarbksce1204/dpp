import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner input=new Scanner(System.in);
		int n=input.nextInt();
		for(int i=0;i<n;i++){
		    int s=input.nextInt();
		    if(s<=70){
		        System.out.println(0);
		    }
		    else if(s>70&&s<=100){
		        System.out.println(500);
		    }
		    else{
		        System.out.println(2000);
		    }
		}

	}
}
