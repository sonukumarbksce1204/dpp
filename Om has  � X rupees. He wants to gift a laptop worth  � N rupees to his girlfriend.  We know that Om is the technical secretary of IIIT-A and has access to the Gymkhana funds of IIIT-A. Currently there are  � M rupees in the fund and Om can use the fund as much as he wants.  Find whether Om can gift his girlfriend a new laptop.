import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner input=new Scanner(System.in);
		int x=input.nextInt();
		int n=input.nextInt();
		int m=input.nextInt();
        System.out.println(x+m>=n?"YES":"NO");		

	}
}
