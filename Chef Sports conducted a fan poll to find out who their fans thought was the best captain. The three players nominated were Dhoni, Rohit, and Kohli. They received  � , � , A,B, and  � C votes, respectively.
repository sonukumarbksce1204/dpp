import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner input=new Scanner(System.in);
		int x=input.nextInt();
		int y=input.nextInt();
		int z=input.nextInt();
		if(x>y&&x>z){
		    System.out.println("YES");
		}
		else{
		    System.out.println("NO");
		}

	}
}
