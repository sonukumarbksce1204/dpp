import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner input=new Scanner(System.in);
		int n=input.nextInt();
		while(n-- >0){
		    int x=input.nextInt();
		    System.out.print(x>1?((x%2==0)?x/2:(x+1)/2):x);
		    System.out.println(" "+x);
		}

	}
}
