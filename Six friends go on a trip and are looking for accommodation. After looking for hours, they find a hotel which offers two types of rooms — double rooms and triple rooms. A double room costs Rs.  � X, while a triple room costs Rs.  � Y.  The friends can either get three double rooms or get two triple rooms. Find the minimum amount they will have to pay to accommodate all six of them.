import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner input=new Scanner(System.in);
		int n=input.nextInt();
		for(int i=0;i<n;i++){
		    int x=input.nextInt();
		    int y=input.nextInt();
		    x*=3;
		    y*=2;
		    System.out.println(Math.min(x,y));
		}

	}
}
