import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner input=new Scanner(System.in);
		int n=input.nextInt();
		for(int i=0;i<n;i++){
		    int x=input.nextInt();
		    if(x>=4&&x<7){
		        System.out.println("MEDIUM");
		    }
		    else if(x<4){
		        System.out.println("MILD");
		    }
		    else{
		        System.out.println("HOT");
		    }
		}

	}
}
