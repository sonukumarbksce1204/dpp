import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner input=new Scanner(System.in);
		int n=input.nextInt();
		while(n-- >0){
		    int t=input.nextInt();
		    int[] arr=new int[t];
		    int val=0;
		    for(int i=0;i<t;i++){
		        arr[i]=input.nextInt();
		        if(arr[i]>=1000){
		            val++;
		        }
		    }
		    System.out.println(val);
		}

	}
}
