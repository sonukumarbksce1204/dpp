import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
	    Scanner input=new Scanner(System.in);
	    int n=input.nextInt();
	    for(int i=0;i<n;i++){
	        int m=input.nextInt();
	        int t=input.nextInt();
	        if(t>=m){
	            int c=Math.round(t/m);
	            System.out.println(c);
	        }
	        else{
	            System.out.println(0);
	        }
	    }

	}
}
