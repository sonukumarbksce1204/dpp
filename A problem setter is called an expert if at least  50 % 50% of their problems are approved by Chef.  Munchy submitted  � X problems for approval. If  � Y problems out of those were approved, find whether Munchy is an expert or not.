import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner input=new Scanner(System.in);
		int n=input.nextInt();
		while(n-- >0){
		    double x=input.nextInt();
		    double y=input.nextInt();
		    System.out.println((y/x)*100.0>=50?"YES":"NO");
		}

	}
}
