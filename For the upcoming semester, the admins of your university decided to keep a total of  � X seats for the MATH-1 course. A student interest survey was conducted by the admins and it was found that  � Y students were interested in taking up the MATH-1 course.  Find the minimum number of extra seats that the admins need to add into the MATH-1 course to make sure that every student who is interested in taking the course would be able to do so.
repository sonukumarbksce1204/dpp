import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner input=new Scanner(System.in);
		int n=input.nextInt();
		for(int i=0;i<n;i++){
		    int x=input.nextInt();
		    int y=input.nextInt();
		    if(y>=x){
		        System.out.println(y-x);
		    }
		    else{
		        System.out.println(0);
		    }
		}

	}
}
