import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner input=new Scanner(System.in);
		int n=input.nextInt();
		while(n-- >0){
		    int l=input.nextInt();
		    int a=input.nextInt();
		    int count=0;
		    int[] arr=new int[l];
		    for(int i=0;i<l;i++){
		        arr[i]=input.nextInt();
		        if(arr[i]>=a){
		            count++;
		        }
		    }
		    System.out.println(count);
		}

	}
}
