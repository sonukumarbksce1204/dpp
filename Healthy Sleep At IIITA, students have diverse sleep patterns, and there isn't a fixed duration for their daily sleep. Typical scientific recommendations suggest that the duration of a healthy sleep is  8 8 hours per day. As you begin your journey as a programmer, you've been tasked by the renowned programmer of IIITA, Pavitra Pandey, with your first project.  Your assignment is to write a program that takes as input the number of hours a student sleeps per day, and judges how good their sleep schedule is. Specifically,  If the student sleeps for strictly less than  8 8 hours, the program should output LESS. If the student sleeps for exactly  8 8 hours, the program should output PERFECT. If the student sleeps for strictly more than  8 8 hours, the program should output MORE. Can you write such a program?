import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
		Scanner input=new Scanner(System.in);
        int n=input.nextInt();
        if(n<8){
        System.out.println("LESS");
        }else if(n==8){
        System.out.println("PERFECT");
        }else{
        System.out.println("MORE");
        }

	}
}
